<view class="cash_detail"><uni-nav-bar vue-id="7e5a261d-1" left-icon="arrowleft" title="现金劵信息" status-bar="{{navBar}}" fixed="true" data-event-opts="{{[['^clickLeft',[['leftClick']]]]}}" onClickLeft="__e" onVueInit="__l"></uni-nav-bar><view class="information"><view class="weight fourteen"><block a:if="{{detailList.coupons_status==2}}"><text>正常</text></block><block a:if="{{detailList.coupons_status==1}}"><text>未开始</text></block><block a:if="{{detailList.coupons_status==3}}"><text>已使用</text></block><block a:if="{{detailList.coupons_status==4}}"><text>已过期</text></block></view><view class="keys_str"><view><view>标签</view><view><block a:for="{{detailList.coupons_label}}" a:for-item="item" a:for-index="__i0__"><text class="{{((((('cash_se')+' '+((detailList.coupons_status==2)?'cash_se2':''))+' '+((detailList.coupons_status==1)?'cash_se1':''))+' '+((detailList.coupons_status==3)?'cash_se3':''))+' '+((detailList.coupons_status==4)?'cash_se4':''))}}">{{item}}</text></block></view></view><view><view>名称</view><view>{{detailList.coupons_title}}</view></view><view><view>面值</view><view>{{detailList.money+"元"}}</view></view><view><view>余额</view><view>{{detailList.residue+"元"}}</view></view><view><view>开始时间</view><view>{{detailList.start_time}}</view></view><view><view>结束时间</view><view>{{detailList.expire_time}}</view></view><view><view>描述</view><view>{{detailList.describe}}</view></view></view></view><view class="usage_record"><view class="record_title">使用记录</view><view><block a:for="{{detailList.details}}" a:for-item="item" a:for-index="index"><view class="record_info"><text>{{item.order_sn}}</text><text>{{item.spend+"元"}}</text><text>{{item.created_at}}</text></view></block></view><block a:if="{{exist}}"><view class="bitmap"><image src="../../static/img/cash_record.png" mode></image><text class="ten">无记录</text></view></block></view></view>