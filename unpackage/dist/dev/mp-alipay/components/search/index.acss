
.my_search {
	background: white;
	padding: 20rpx 0;
	width: 100%;
	position: fixed;
	z-index: 3;
}
.my_search_content {
	background: #f7f6f6;
	height: 68rpx;
	margin: 0 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	border-radius: 50rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.my_search_content .text {
	color: #d6cdd2;
}


