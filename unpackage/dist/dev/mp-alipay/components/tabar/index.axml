<view><view hidden="{{!(masktabar)}}" style="position:fixed;width:100vw;height:60px;z-index:9999;bottom:0;"></view><view style="height:60px;"></view><view class="my_tabar"><block a:for="{{tabList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['pageUrl',['$0'],[[['tabList','',index]]]]]]]}}" class="single_tabar" onTap="__e"><block a:if="{{tabarIndex==index}}"><image class="image" src="{{item.img}}" mode="aspectFit"></image></block><block a:else><image class="image" src="{{item.inImg}}" mode="aspectFit"></image></block><text class="{{(tabarIndex==index?'color':'incolor')}}">{{item.title}}</text></view></block></view></view>