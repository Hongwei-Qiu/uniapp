<view class="my_profile flex"><view data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" class="photo" onTap="__e"><block a:if="{{config.logo&&config.shuiyin==1}}"><image class="shuiyin" src="{{config.logo}}" mode="aspectFit"></image></block><image class="good_img" src="{{ware.img==''?imgRemote+config.item_default:ware.img}}" mode="aspectFit"></image></view><view class="info"><view class="operate flex"><view data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" style="width:86%;" onTap="__e"><view>{{ware.title}}</view><block a:if="{{ware.describe}}"><view class="hidden gray_font twelve">{{ware.describe}}</view></block></view><block a:if="{{url=='collect'}}"><view data-event-opts="{{[['tap',[['cancelCollect',['$event']]]]]}}" onTap="__e"><text class="iconfont icon-alreadystar" style="color:orange;"></text></view></block></view><view class="flex_left_right"><view data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" style="width:82%;" onTap="__e"><view><block a:for="{{ware.label}}" a:for-item="item" a:for-index="index" a:key="index"><text class="red_tag">{{item}}</text></block></view><block a:if="{{token}}"><block><block a:if="{{config.is_look==1}}"><block><block a:if="{{ware.attr.length}}"><view class="hidden"><text class="red_font">{{"￥"+ware.area_price+"/"+ware.unit}}</text><text class="gray_font">(多规格)</text></view></block><block a:else><view class="red_font"><block a:if="{{ware.activity_num>=ware.cart_num&&ware.is_activity==1}}"><block>{{'￥'+(ware.activity_price+'/'+ware.unit)+''}}</block></block><block a:else><block>{{'￥'+(ware.price+'/'+ware.unit)}}</block></block></view></block></block></block><block a:else><block><view class="red_font">￥***</view></block></block></block></block><block a:else><block><block a:if="{{ware.attr.length}}"><view><text class="red_font">{{"￥"+ware.area_price+"/"+ware.unit}}</text><text class="gray_font">(多规格)</text></view></block><block a:else><view class="red_font">{{'￥'+ware.price+"/"+ware.unit+''}}</view></block></block></block></view><view class="align_center"><block a:if="{{ware.attr.length}}"><block><image class="add_cart" src="../../static/img/addcart.png" data-event-opts="{{[['tap',[['showCart',['$event']]]]]}}" onTap="__e"></image></block></block><block a:else><block><block a:if="{{ware.cart_num}}"><my-stepper vue-id="14f0898a-1" val="{{ware.cart_num}}" data-event-opts="{{[['^showKey',[['showKey']]],['^minus',[['minus',[ware.cart_num-1]]]],['^plus',[['plus',[ware.cart_num+1]]]]]}}" onShowKey="__e" onMinus="__e" onPlus="__e" onVueInit="__l"></my-stepper></block><block a:else><image class="add_cart" src="../../static/img/plus.png" data-event-opts="{{[['tap',[['plusCart',['$event']]]]]}}" onTap="__e"></image></block></block></block></view></view></view></view>