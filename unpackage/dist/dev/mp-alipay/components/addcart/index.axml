<view class="my_addcart"><view class="top"><view class="info"><view class="detail"><view><block a:if="{{config.logo&&config.shuiyin==1}}"><image class="shuiyin position_a" src="{{config.logo}}" mode="aspectFit"></image></block><image class="good_img" src="{{cartware.img==''?imgRemote+config.item_default:cartware.img}}" mode="aspectFit"></image></view><view class="cart_ware_info"><view><view>{{cartware.title}}</view><block a:if="{{cartware.describe}}"><view class="hidden describe">{{cartware.describe}}</view></block></view><view><view><block a:for="{{cartware.label}}" a:for-item="item" a:for-index="index" a:key="index"><text class="red_tag">{{item}}</text></block></view><block a:if="{{token}}"><block><block a:if="{{config.is_look==1}}"><block><block a:if="{{cartware.attr.length}}"><block><block a:if="{{cartware.attr[kind].is_activity==1}}"><block><view><text class="red_font">{{"￥"+cartware.attr[kind].activity_price+"/"+cartware.attr[kind].unit}}</text><text class="line_through ten">{{"￥"+cartware.attr[kind].attr_price}}</text></view></block></block><block a:else><block><block a:if="{{cartware.attr[kind].market_price==1}}"><view class="red_font">时价</view></block><block a:else><view class="red_font">{{"￥"+cartware.attr[kind].attr_price+"/"+cartware.attr[kind].unit}}</view></block></block></block></block></block><block a:else><block><block a:if="{{cartware.is_activity==1}}"><block><view><text class="red_font">{{"￥"+cartware.activity_price+"/"+cartware.unit}}</text><text class="line_through ten">{{"￥"+cartware.price+"/"+cartware.unit}}</text></view></block></block><block a:else><block><block a:if="{{cartware.market_price==1}}"><view class="red_font">时价</view></block><block a:else><view class="red_font">{{'￥'+cartware.price+"/"+cartware.unit+''}}</view></block></block></block></block></block></block></block><block a:else><block><view class="red_font">￥***</view></block></block></block></block><block a:else><block><block a:if="{{cartware.attr.length}}"><view class="red_font"><block a:if="{{cartware.attr[kind].market_price==1}}"><text>时价</text></block><block a:else><text>{{"￥"+cartware.attr[kind].attr_price+"/"+cartware.attr[kind].unit}}</text></block></view></block><block a:else><view class="red_font"><block a:if="{{cartware.market_price==1}}"><text>时价</text></block><block a:else><text>{{"￥"+cartware.price+"/"+cartware.unit}}</text></block></view></block></block></block></view></view></view><text data-event-opts="{{[['tap',[['onClose',['$event']]]]]}}" class="iconfont icon-X" onTap="__e"></text></view><block a:if="{{cartware.attr.length}}"><view class="spec"><view class="title">选择规格</view><view class="select"><block a:for="{{cartware.attr}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['selectSpec',[index]]]]]}}" class="{{(('label')+' '+(index==kind?'active':''))}}" onTap="__e">{{item.attr_title}}</view></block></view></view></block><view class="buy_num"><view>购买数量</view><view><my-stepper vue-id="44671aa3-1" val="{{value}}" min="1" data-event-opts="{{[['^showKey',[['showKey']]],['^minus',[['minus']]],['^plus',[['plus']]]]}}" onShowKey="__e" onMinus="__e" onPlus="__e" onVueInit="__l"></my-stepper></view></view></view><view data-event-opts="{{[['tap',[['determine',['$event']]]]]}}" class="determine" onTap="__e">确定</view><block a:if="{{isTop}}"><my-keyboard vue-id="44671aa3-2" arrObj="{{arrObj}}" data-event-opts="{{[['^cancelKey',[['e0']]],['^toParent',[['toParent']]]]}}" onCancelKey="__e" onToParent="__e" onVueInit="__l"></my-keyboard></block></view>