<view class="{{(('s-tabs _div')+' '+customClass)}}"><view class="s-tabs-nav-wrap _div" style="{{('height:'+height+'rpx')}}"><scroll-view class="scroll-wrap" show-scrollbar="{{false}}" scroll-with-animation="{{true}}" scroll-x="{{true}}" scroll-left="{{scrollLeft}}"><view class="scroll-view _div"><view class="s-tab-nav-view _div"><block a:if="{{!slotTitle}}"><block a:for="{{navInfoList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['navClick',[index,'$0'],[[['navInfoList','',index]]]]]]]}}" class="{{(((('_div')+' '+'s-tab-nav')+' '+[(item.disabled)?'is-disabled':''])+' '+[(active==index)?'is-active':''])}}" style="{{(navWidth+'color:'+(active==index?activeColor:color))}}" onTap="__e">{{item.title}}</view></block></block><block a:else><slot></slot></block></view><block a:if="{{line}}"><view class="s-tab-line _div" style="{{'width:'+(lineWidth+'px')+';'+('height:'+(lineHeight+'rpx')+';')+('background:'+(lineColor)+';')+('transform:'+('translateX('+lineLeft+'px)')+';')}}"></view></block></view></scroll-view></view><block a:if="{{!slotTitle}}"><view class="s-tabs-content-wrap _div" style="{{(transform+transition)}}"><slot></slot></view></block></view>