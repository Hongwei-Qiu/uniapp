<view class="accountedit"><uni-nav-bar vue-id="f8ac0bcc-1" left-icon="arrowleft" title="账号编辑" status-bar="{{navBar}}" fixed="true" right-text="删除" data-event-opts="{{[['^clickLeft',[['leftClick']]],['^clickRight',[['deleteAccount']]]]}}" bind:clickLeft="__e" bind:clickRight="__e" bind:__l="__l"></uni-nav-bar><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="get_info"><view><text>账号名称</text><input name="nickname" type="text" placeholder="联系人姓名" placeholder-class="place_style" value="{{childInfo.nickname}}"/></view><view><text>联系人</text><input name="contact" type="text" placeholder="请输入联系人" placeholder-class="place_style" value="{{childInfo.contact}}"/></view><view><text>手机号</text><input name="mobile" type="number" placeholder="请输入手机号" maxlength="11" placeholder-class="place_style" value="{{childInfo.phone}}"/></view><view><text>密码</text><input name="password" type="password" placeholder="请输入六位及以上的密码" placeholder-class="place_style" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="flex_left_right"><view style="color:#1EA55A;">账号启用和禁用</view><evan-switch vue-id="f8ac0bcc-2" active-color="#1EA55A" value="{{checked}}" data-event-opts="{{[['^change',[['openStatu']]],['^input',[['__set_model',['','checked','$event',[]]]]]]}}" bind:change="__e" bind:input="__e" bind:__l="__l"></evan-switch></view><view data-event-opts="{{[['tap',[['mapPage',['$event']]]]]}}" bindtap="__e"><text>收货地址</text><view class="flex_left_right" style="width:83%;"><text class="hidden2">{{childInfo.address}}</text><uni-icons vue-id="f8ac0bcc-3" type="arrowright" size="18" color="gray" bind:__l="__l"></uni-icons></view></view><view><text>门牌号</text><input name="details" type="text" placeholder="例如：5号509室" placeholder-class="place_style" value="{{childInfo.details}}"/></view></view><button class="submit_button button_style" form-type="submit">保存</button></form></view>