<view class="accountadd"><uni-nav-bar vue-id="3fb2ee37-1" left-icon="arrowleft" title="账号添加" status-bar="{{navBar}}" fixed="true" data-event-opts="{{[['^clickLeft',[['leftClick']]]]}}" bind:clickLeft="__e" bind:__l="__l"></uni-nav-bar><view class="get_info"><view><text>账号名称</text><input type="text" placeholder="联系人姓名" placeholder-class="place_style" data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]]]]]}}" value="{{nickname}}" bindinput="__e"/></view><view><text>联系人</text><input type="text" placeholder="请输入联系人" placeholder-class="place_style" data-event-opts="{{[['input',[['__set_model',['','contact','$event',[]]]]]]}}" value="{{contact}}" bindinput="__e"/></view><view><text>手机号</text><input type="number" placeholder="请输入手机号" maxlength="11" placeholder-class="place_style" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]]]]]}}" value="{{mobile}}" bindinput="__e"/></view><view><text>密码</text><input password placeholder="请输入六位及以上的密码" placeholder-class="place_style" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['mapPage',['$event']]]]]}}" bindtap="__e"><text>收货地址</text><view class="flex_left_right" style="width:83%;"><text class="hidden2">{{address}}</text><uni-icons vue-id="3fb2ee37-2" type="arrowright" size="18" color="gray" bind:__l="__l"></uni-icons></view></view><view><text>门牌号</text><input type="text" placeholder="例如：5号509室" placeholder-class="place_style" data-event-opts="{{[['input',[['__set_model',['','details','$event',[]]]]]]}}" value="{{details}}" bindinput="__e"/></view></view><view data-event-opts="{{[['tap',[['formSubmit',['$event']]]]]}}" class="submit_button button_style" bindtap="__e">保存</view></view>