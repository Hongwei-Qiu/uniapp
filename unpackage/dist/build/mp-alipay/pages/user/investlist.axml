<view class="invest_list"><uni-nav-bar vue-id="e915baa4-1" left-icon="arrowleft" title="充值记录" status-bar="{{navBar}}" fixed="true" data-event-opts="{{[['^clickLeft',[['leftClick']]]]}}" onClickLeft="__e" onVueInit="__l"></uni-nav-bar><block a:if="{{bitmap}}"><view class="all_account"><view class="flex_left_right single" a:for="{{rechargeList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="account_info"><view><block a:if="{{item.consume==1&&item.type==1}}"><text>{{"订单消费："+item.order_sn}}</text></block><block a:if="{{item.consume==1&&item.type==3}}"><text class="bottom ">{{"后台提现："+item.order_sn}}</text></block><block a:if="{{item.consume==0&&item.type==2}}"><text class="bottom ">{{"前台充值："+item.order_sn}}</text></block><block a:if="{{item.consume==0&&item.type==1}}"><text class="bottom ">{{"后台充值："+item.order_sn}}</text></block></view><view class="gray_font"><block a:if="{{item.consume==1&&item.type==1}}"><text>{{'消费时间：'+item.date}}</text></block><block a:if="{{item.consume==0}}"><text class="gray">{{"充值时间："+item.date}}</text></block><block a:if="{{item.consume==1&&item.type==3}}"><text class="gray">{{"提现时间："+item.date}}</text></block></view></view><block a:if="{{item.consume==1}}"><view>{{"-"+item.price}}</view></block><block a:if="{{item.consume==0}}"><view class="red_font">{{"+"+item.price}}</view></block></view><my-loading vue-id="e915baa4-2" loading="{{loading}}" onVueInit="__l"></my-loading></view></block><block a:else><view class="bitmap"><image src="../../static/img/no_record.png" mode="aspectFit"></image></view></block></view>