<view class="bill_detail"><uni-nav-bar vue-id="66c31e6e-1" left-icon="arrowleft" title="账单详情" status-bar="{{navBar}}" fixed="true" data-event-opts="{{[['^clickLeft',[['leftClick']]]]}}" onClickLeft="__e" onVueInit="__l"></uni-nav-bar><block a:if="{{waitLoad}}"><view><view class="method_way"><block a:if="{{moneyListInfo.pay_type==1}}"><view style="color:#009a44;">余额</view></block><block a:if="{{moneyListInfo.pay_type==2}}"><view style="color:#52ca28;">现金</view></block><block a:if="{{moneyListInfo.pay_type==3}}"><view style="color:#02B1E4;">转账</view></block><view class="twelve gray_font">{{"账单编号 : "+moneyListInfo.moneySn}}</view><view class="twelve gray_font">{{"账单创建时间 : "+moneyListInfo.date}}</view></view><view class="order_num"><view>{{"订单数量 ("+num+")"}}</view><view class="twelve flex_left_right" a:for="{{moneyListInfo.list}}" a:for-item="item" a:for-index="index" a:key="index"><text>{{"订单编号 : "+item.order_sn}}</text><text class="red_font">{{"¥"+item.totalPrice}}</text></view><view class="twelve text_right">总额 :<text class="red_font">{{"¥"+moneyListInfo.total}}</text></view></view><view class="pay_money twelve"><view class="flex_left_right"><text>实收</text><text class="red_font">{{"¥"+moneyListInfo.net_receipts}}</text></view><view class="flex_left_right"><text>差额</text><text class="red_font">{{"¥"+moneyListInfo.difference}}</text></view><view class="flex_left_right"><text>应收</text><text class="red_font">{{"¥"+moneyListInfo.receivable}}</text></view></view></view></block></view>