<view class="bill"><uni-nav-bar vue-id="dfb6c20c-1" left-icon="arrowleft" title="账单记录" status-bar="{{navBar}}" fixed="true" data-event-opts="{{[['^clickLeft',[['leftClick']]]]}}" onClickLeft="__e" onVueInit="__l"></uni-nav-bar><view class="search_style"><view data-event-opts="{{[['tap',[['openCalendar',['$event']]]]]}}" class="my_search_content" onTap="__e"><text class="iconfont icon-sousuo"></text><text style="{{'color:'+(date!='请选择日期'?'black':'#d6cdd2')+';'}}">{{date}}</text></view></view><view style="width:100%;height:100rpx;position:fixed;background:#f7f6f9;"></view><block a:if="{{bitmap}}"><view class="all_account"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['detailPage',['$0'],[[['list','',index,'id']]]]]]]}}" class="flex_left_right single" onTap="__e"><view class="account_info"><block a:if="{{item.pay_type==1}}"><view style="color:#009a44;">余额</view></block><block a:if="{{item.pay_type==2}}"><view style="color:#52ca28;">现金</view></block><block a:if="{{item.pay_type==3}}"><view style="color:#02B1E4;">转账</view></block><view class="gray_font">{{item.date}}</view></view><view class="red_font">{{"¥"+item.receivable}}</view></view></block><my-loading vue-id="dfb6c20c-2" loading="{{loading}}" onVueInit="__l"></my-loading></view></block><block a:else><view class="bitmap"><image src="../../static/img/no_record.png" mode="aspectFit"></image></view></block><uni-calendar vue-id="dfb6c20c-3" insert="{{false}}" lunar="{{true}}" range="{{true}}" ref="__r" data-ref="calendar" data-event-opts="{{[['^confirm',[['confirm']]]]}}" onConfirm="__e" onVueInit="__l"></uni-calendar></view>