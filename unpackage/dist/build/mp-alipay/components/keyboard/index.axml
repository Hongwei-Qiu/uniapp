<view class="my_keyboard"><view class="inputval"><block a:if="{{invest}}"><view style="padding-top:20rpx;">充值金额 (元)</view></block><input class="getval" type="text" disabled="true" placeholder="请输入..." placeholder-style="font-size:28rpx;" data-event-opts="{{[['input',[['__set_model',['','val','$event',[]]]]]]}}" value="{{val}}" onInput="__e"/></view><view class="keyboard"><view class="left"><view class="num" id="{{index}}" data-event-opts="{{[['tap',[['changeNum',[index+1]]]]]}}" onTap="__e" a:for="{{9}}" a:for-item="item" a:for-index="index" a:key="index">{{index+1}}</view><view data-event-opts="{{[['tap',[['changeNum',[0]]]]]}}" class="num" onTap="__e">0</view><view data-event-opts="{{[['tap',[['changeNum',['.']]]]]}}" class="num" onTap="__e">.</view><view data-event-opts="{{[['tap',[['changeNum',['delete']]]]]}}" class="shanchu num" onTap="__e"><image class="delte" src="/static/img/shanchu.png" mode="aspectFit"></image></view></view><view class="right"><view><view data-event-opts="{{[['tap',[['clean',['$event']]]]]}}" class="option_button" onTap="__e">清除</view><view data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="option_button" onTap="__e">取消</view><view data-event-opts="{{[['tap',[['toParent',['$event']]]]]}}" class="determine option_button" onTap="__e">确认</view></view></view></view></view>